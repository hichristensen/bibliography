@InProceedings{	  burschka_v-gpsslam_2004,
  title =        {V-{GPS}({SLAM}): {Vision}-based {Inertial} {System} for
                  {Mobile} {Robots}},
  abstract =     {We present a novel vision-based approach to simultaneous
                  localization and mapping (SLAM). We discuss it in the context
                  of estimating the 6 DoF pose of a mobile robot from the
                  perception of a monocular camera using a minimum set of three
                  natural landmarks. In contrast to our previously presented
                  V-GPS system, which navigates based on a set of known
                  landmarks, the current approach allows to estimate the
                  required information about the landmarks on-the-fly during the
                  exploration of an unknown environment The method is applicable
