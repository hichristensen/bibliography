@Article{	  burt_smart_1988,
  title =        {Smart sensing within a pyramid vision machine},
  volume =       76,
  abstract =     {In human vision, sensing is a distinctly dynamic process: the
                  eyes move through an evolving sequence of fixations to explore
                  the visual world, and to selectively gather information
                  critical to the task at hand. Computer vision systems must
                  follow similar smart sensing strategies if they are to perform
                  useful tasks in real time. Through intelligent selection of
                  data at an early analysis stage, systems can reduce the
                  overall computational cost by orders of magnitude. Through
                  smart sensing a vision system makes efficient use of limited
                  computing resources. We have built a machine, based on a
                  pyramid architecture, that supports smart sensing and related
                  highly efficient processing. Key elements of the design are a)
                  hierarchical data structures for image representation, b)
                  fine-to-coarse algorithms for the fast generation of image
                  measures, c) coarse-tofine search strategies that rapidly
                  locate objects or events within a scene, and d) high level
                  control mechanisms that guide data gathering even as visual
                  information is being interpreted. This system, known as the
                  Pyramid Vision Machine, achieves high performance at modest
                  cost. Design considerations and several applications are
                  described here.},
  number =       8,
  journal =      {Proceedings of the IEEE},
  author =       {Burt, P.J.},
  month =        aug,
  year =         1988,
  pages =        {1006--1015}
}
