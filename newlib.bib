@InProceedings{	  hou_saliency_2007,
  title =        {Saliency {Detection}: {A} {Spectral} {Residual} {Approch}},
  abstract =     {The ability of human visual system to detect visual saliency
                  is extraordinarily fast and reliable. However, com- putational
                  modeling of this basic intelligent behavior still remains a
                  challenge. This paper presents a simple method for the visual
                  saliency detection. Our model is independent of features,
                  categories, or other forms of prior knowledge of the objects.
                  By analyz- ing the log-spectrum of an input image, we extract
                  the spec- tral residual of an image in spectral domain, and
                  propose a fast method to construct the corresponding saliency
                  map in spatial domain. We test this model on both natural
                  pictures and artificial images such as psychological patterns.
