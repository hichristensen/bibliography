@InProceedings{	  isard_bramble_2001,
  address =      {Vancouver, British Columbia, Canada},
  title =        {{BraMBLe}: {A} {Bayesian} {Multiple}-{Blob} {Tracker}},
  volume =       2,
  url =
                  {http://research.microsoft.com/users/misard/papers/bramble.ps.gz},
  booktitle =    {{ICCV}},
  publisher =    {IEEE Computer Society, Los Alamos CA},
  author =       {Isard, Michael and MacCormick, John},
  year =         2001,
  pages =        {34--41},
  annote =       {This paper presents a new approach for Bayesian multi-object
                  tracking using particle filters. Two novelties are introduced:
                  First, a multi-blob likelihood function which assigns directly
                  comparable likelihoods to hypotheses containing different
                  numbers of objects. Second, a Bayesian multi-object filter
                  which uses the abovementioned multi-blob likelihood function
                  as the observation model. Using the standard machinery of
                  particle filters, this results in a filter which is able to
                  track an unknown and varying number of objects. the paper is
                  very well written and easily understandable though the
                  presented ideas are not trivial at all.}
}
@Article{	  isard_condensation_1998,
  title =        {Condensation – conditional density propagation for visual
                  tracking},
  volume =       29,
  number =       1,
  journal =      {IJCV},
  author =       {Isard, Michael and Blake, Andrew},
  year =         1998,
  keywords =     {CONDENSATION, Contour-Based Object Tracking, Probabilistic
                  Object Tracking},
  pages =        {5--28},
  annote =       {Primary reference for the CONDENSATION algorithm, a sequential
                  Monte Carlo method for visual tracking. In contrast to
                  tracking techniques based on Kalman filter, this novel
                  approach uses sequential importance sampling for conditional
                  propagation of non-Gaussian probability densities. After
                  introducing the principles of probabilistic object tracking,
                  the authors introduce CONDENSATION for dealing with
                  non-Gaussian observations in temporal image sequences. The
                  article concludes with several practical examples for the
                  strengths of particle fitlering in the context of
                  probabilistic object tracking. Recommended as an entry point
                  for CONDENSATION/particle filter based object tracking.}
}
