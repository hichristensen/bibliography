@Article{	  mei_efficient_2008,
  title =        {Efficient {Homography}-{Based} {Tracking} and 3-{D}
                  {Reconstruction} for {Single}-{Viewpoint} {Sensors}},
  volume =       24,
  abstract =     {This paper addresses the problem of motion estimation and 3-D
                  reconstruction through visual tracking with a single viewpoint
                  sensor and in particular how to generalise tracking to
                  calibrated omnidirectional cameras.We analyse different
                  minimisation approaches for the intensity-based cost function
                  (sum-ofsquared differences). In particular, we propose novel
                  variants of the efficient second-order minimisation (ESM) with
                  better computational complexities and compare these algorithms
                  with the inverse composition (IC) and the hyperplane
                  approximation (HA). Issues regarding the use of the IC and HA
                  for 3-D tracking are discussed. We show that even though an
                  iteration of ESM is more computationally expensive than an
                  iteration of IC, the faster convergence rate makes it globally
                  faster. The tracking algorithm was validated using an
                  omnidirectional sensor mounted on a mobile robot.},
  number =       6,
  journal =      {TRO},
