@Article{	  toet_effects_2008,
  title =        {Effects of field-of-view restriction on manoeuvring in a 3-{D}
                  environment.},
  volume =       51,
  abstract =     {Field-of-view (FOV) restrictions are known to affect human
                  behaviour and to degrade performance for a range of different
                  tasks. However, the relationship between human locomotion
                  performance in complex environments and FOV size is currently
                  not fully known. This paper examined the effects of FOV
                  restrictions on the performance of participants manoeuvring
                  through an obstacle course with horizontal and vertical
                  barriers. All FOV restrictions tested (the horizontal FOV was
                  either 30? 75?or 120? while the vertical FOV was always 48?
                  significantly reduced performance compared to the unrestricted
                  condition. Both the time and the number of footsteps needed to
                  traverse the entire obstacle course increased with a
                  decreasing FOV size. The relationship between FOV restriction
                  and manoeuvring performance that was determined can be used to
                  formulate requirements for FOV restricting devices that are
                  deployed to perform time-limited human locomotion tasks in
                  complex structured environments, such},
  number =       3,
  journal =      {Ergonomics},
  author =       {Toet, A. and Jansen, S. E. M. and Delleman, N. J.},
  year =         2008,
  pages =        {385--394}
}
