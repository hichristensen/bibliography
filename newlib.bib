@Article{	  nelson_visual_1991,
  title =        {Visual homing using an associative memory},
  volume =       65,
  abstract =     {Homing is the process by which an autonomous system guides
                  itself to a particular location on the basis of sensory input.
                  In this paper, a method of visual homing using an associative
                  memory based on a simple pattern classifier is described.
                  Homing is accomplished without the use of an explicit world
                  model by utilizing direct associations between learned visual
                  patterns and system motor commands. The method is analyzed in
                  terms of a pattern space and conditions obtained that allow
