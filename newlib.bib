@InProceedings{	  kaess_compact_2003,
  address =      {Coimbra, Portugal},
  title =        {Compact {Encoding} of {Robot}-{Generated} {3D} {Maps} for
                  {Efficient} {Wireless} {Transmission}},
  abstract =     {This work focuses on real-time compression of laser data on
