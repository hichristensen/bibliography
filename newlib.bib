@InProceedings{	  eade_monocular_2007,
  title =        {Monocular {SLAM} as a {Graph} of {Coalesced} {Observations}},
  abstract =     {We present a monocular SLAM system that avoids inconsistency
                  by coalescing observations into independent local coordinate
                  frames, building a graph of the local frames, and optimizing
                  the resulting graph. We choose coordinates that minimize the
                  nonlinearity of the updates in the nodes, and suggest a
                  heuristic measure of such nonlinearity, using it to guide our
                  traversal of the graph. The system operates in real-time on
                  sequences with several hundreds of landmarks while performing
                  global graph optimization, yielding accurate and nearly
                  consistent estimation relative to offline bundle adjustment,
                  and considerably better consistency than EKF SLAM and
                  FastSLAM.},
  booktitle =    {{ICCV}},
  author =       {Eade, E. and Drummond, T.},
  month =        oct,
  year =         2007,
  note =         {Place: Rio de Janeiro, Brazil}
}
@InProceedings{	  eade_scalable_2006,
  title =        {Scalable {Monocular} {SLAM}},
  abstract =     {Localization and mapping in unknown environments becomes more
                  difficult as the complexity of the environment increases. With
                  conventional techniques, the cost of maintaining estimates
                  rises rapidly with the number of landmarks mapped. We present
                  a monocular SLAM system that employs a particle filter and
                  top-down search to allow realtime performance while mapping
                  large numbers of landmarks. To our knowledge, we are the first
                  to apply this FastSLAM-type particle filter to single-camera
                  SLAM. We also introduce a novel partial initialization
                  procedure that efficiently determines the depth of new
                  landmarks. Moreover, we use information available in
                  observations of new landmarks to improve camera pose
                  estimates. Results show the system operating in real-time on a
                  standard workstation while mapping hundreds of landmarks.},
  booktitle =    {{CVPR}},
  author =       {Eade, E. and Drummond, T.},
  month =        jun,
  year =         2006,
  note =         {Place: New York, NY}
}
