@article{hafting05:_micros,
annote = {The ability to find one's way depends on neural
algorithms that integrate information about place,
distance and direction, but the implementation of
these operations in cortical microcircuits is poorly
understood. Here we show that the dorsocaudal medial
entorhinal cortex \{dMEC\} contains directionally
oriented, topographically organised neural map of
the spatial environment. Its key unit is the "grid
cell" which is activated whenever the animal's
position coincides with any vertex of a regular grid
of equilateral trriangles spanning the surface of
the environment. Grids of neighbouring cells share a
common orientation and spacing, but their certex
locations (their phases) differ. The spacing and
size of individual fields increase from dorsal to
central dMEC. The map is anchored to external
lanndmarks but persists in their absence, suggesting
the grid cells might be part ofa generalised path
integration based map of the spatial environment},
author = {Hafting, T and Fyhn, M and Molden, S and Moser, M.-B. and Moser, E I},
journal = {Nature},
month = aug,
number = {7052},
pages = {801--804},
title = {{Microstructure of a spatial map in the entorhinal cortex}},
volume = {436},
year = {2005}
}
