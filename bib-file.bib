@inproceedings{pacchierotti06:_iros,
	Abstract = {In this paper, the design of an office-guide robot
                  for social interaction studies is presented. We are
                  interested in studying the impact of passage
                  behaviours in casual encounters. While the system
                  offers assistance in locating the appropriate office
                  that a visitor wants to reach, it is expected to
                  engage in a passing behaviour to allow free passage
                  for other persons that it may encounter. Through use
                  of such an approach it is possible to study the
                  effect of social interaction in a situation that is
                  much more natural than out-of-context user
                  studies. The system has been tested in an early
                  evaluation phase when it worked for almost 7
                  hours. A total of 64 interactions with people were
                  registered and 13 passage behaviors were performed
                  to conclude that this framework can be successfully
                  used for the evaluationof passing behaviors in
                  natural contexts of operation. },
	Address = {Beijing, China},
	Annote = { },
	Author = {E. Pacchierotti and H. I. Christensen and P. Jensfelt},
	Booktitle = {Intl Conf on Intelligent Robots and Systems (IROS)},
	Month = {Oct.},
	Organization = {RSJ/IEEE},
	Pdf = {http://www.nada.kth.se/~hic/hic-papers/iros_06_pacchierotti_final.pdf},
	Title = {Design of an Office-Guide Robot for Social Interaction Studies},
	Year = 2006}

@inproceedings{pacchierotti06:_roman,
	Abstract = {Casual encounters with mobile robots for non-
                  experts can be a challenge due to lack of an
                  interaction model. The present work is based on the
                  rules from proxemics which are used to design a
                  passing strategy. In narrow corridors the lateral
                  distance of passage is a key parameter to
                  consider. An implemented system has been used in a
                  small study to verify the basic parametric design
                  for such a system. In total 10 subjects evaluated
                  variations in proxemics for encounters with a robot
                  in a corridor setting. The user feedback indicates
                  that entering the intimate sphere of people is less
                  comfortable, however a too significant avoidance is
                  also considered unnecessary. Adequate signaling of
                  avoidance is a behaviour that must be carefully
                  tuned. },
	Address = {Hertfordshire, UK},
	Author = {E. Pacchierotti and H. I. Christensen and P. Jensfelt},
	Booktitle = {Workshop on Robot and Human Interactive Communication (ROMAN)},
	Editor = {K. Dautenhahn},
	Month = {Sep.},
	Organization = {IEEE},
	Pdf = {http://www.nada.kth.se/~hic/hic-papers/roman06-pacchierotti-final.pdf},
	Title = {Evaluation of Passing Distance for Social Robots},
	Year = 2006}

