@inproceedings{sherrah:01:a,
	Address = {Vancouver, British Columbia, Canada},
	Author = {Jamie Sherrah and Shaogang Gong},
	Booktitle = ICCV,
	Keywords = {modality fusion, tracking, Bayesian network},
	Pages = {42-49},
	Publisher = {IEEE Computer Society, Los Alamos CA},
	Title = {Continuous Global Evidence-Based Bayesian Modality Fusion for Simultaneous Tracking of Multiple Objects},
	Url = {http://www.dcs.qmul.ac.uk/~sgg/papers/iccv01-sherrah-gong.pdf.gz},
	Volume = {2},
	Year = 2001,
	Annote = {Detailed description how one can fuse multiple
                  modalities using Bayesian networks. In contrast to
                  classical cue fusion approaches using a voting
                  approach, continuous Bayesian modality fusion (CBMF)
                  combines multiple cues by reasoning about the
                  individual cue's reliability. Continuous global
                  evidence-based Bayesian modality fusion has four
                  main advantages: 1) Continuous sampling reflects the
                  complexity of Bayesian inference most
                  appropriately. 2) Using global evidence allows to
                  consider all observations from a single modality for
                  inference. Most voting approaches take only a single
                  observation per modality into consideration. 3)
                  Bayesian inference allows to introduce new evidence
                  one by one. This provides the ability to distinguish
                  between mandatory modalities required for object
                  tracking and optional modalities which give only
                  further hints about the presence of an
                  object. Furthermore, the nature of Bayesian modality
                  fusion allows to implement on-demand computation for
                  computationally expensive modalities that are only
                  computet selectively for disambiguating ambigous
                  object localization.},
	Bdsk-Url-1 = {http://www.dcs.qmul.ac.uk/~sgg/papers/iccv01-sherrah-gong.pdf.gz}}

@inproceedings{isard:01:a,
	Address = {Vancouver, British Columbia, Canada},
	Author = {Michael Isard and John MacCormick},
	Booktitle = ICCV,
	Pages = {34-41},
	Publisher = {IEEE Computer Society, Los Alamos CA},
	Title = {BraMBLe: A Bayesian Multiple-Blob Tracker},
	Url = {http://research.microsoft.com/users/misard/papers/bramble.ps.gz},
	Volume = 2,
	Year = 2001,
	Annote = {This paper presents a new approach for Bayesian
                  multi-object tracking using particle filters. Two
                  novelties are introduced: First, a multi-blob
                  likelihood function which assigns directly
                  comparable likelihoods to hypotheses containing
                  different numbers of objects. Second, a Bayesian
                  multi-object filter which uses the abovementioned
                  multi-blob likelihood function as the observation
                  model. Using the standard machinery of particle
                  filters, this results in a filter which is able to
                  track an unknown and varying number of objects. the
                  paper is very well written and easily understandable
                  though the presented ideas are not trivial at all.},
	Bdsk-Url-1 = {http://research.microsoft.com/users/misard/papers/bramble.ps.gz}}

@article{arulampalam:02:a,
	Author = {{M. Sanjeev} Arulampalam and Simon Maskell and Neil Gordon and Tim Clapp},
	Journal = SIGPROC,
	Keywords = {Particle Fitler, Bayesian Tracking, Kalman Filter},
	Month = feb,
	Number = 2,
	Pages = {174-188},
	Title = {A Tutorial on Particle Filters for Online Nonlinear/Non-Gaussian Bayesian Tracking},
	Volume = 50,
	Year = 2002,
	Annote = {Well written tutorial on Sequential Monte Carlo
                  methods for Bayesian tracking. After introducing
                  nonlinear Bayesian tracking, this tutorial discusses
                  two optimal algorithms for solving the tracking
                  problem (Kalman filters and grid-based methods)
                  before focussing on suboptimal algorithms like
                  extended Kalman filters and particle filters. In
                  contrast to the optimal methods, the suboptimal are
                  by far less restrictive with their constraints and
                  are therefore able to cope with the non-Gaussian
                  case. Together with \cite{doucet:02:a}, this
                  tutorial gives a nice overview over state-of-the-art
                  techniques for solving the nonlinear/non-Gaussian
                  Bayesian tracking problem.}}

@book{doucet:02:a,
