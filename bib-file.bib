@inproceedings{isard:01:a,
	Address = {Vancouver, British Columbia, Canada},
	Annote = {This paper presents a new approach for Bayesian
                  multi-object tracking using particle filters. Two
                  novelties are introduced: First, a multi-blob
                  likelihood function which assigns directly
                  comparable likelihoods to hypotheses containing
                  different numbers of objects. Second, a Bayesian
                  multi-object filter which uses the abovementioned
                  multi-blob likelihood function as the observation
                  model. Using the standard machinery of particle
                  filters, this results in a filter which is able to
                  track an unknown and varying number of objects. the
                  paper is very well written and easily understandable
                  though the presented ideas are not trivial at all.},
	Author = {Michael Isard and John MacCormick},
	Booktitle = ICCV,
	Pages = {34-41},
	Publisher = {IEEE Computer Society, Los Alamos CA},
	Title = {BraMBLe: A Bayesian Multiple-Blob Tracker},
	Url = {http://research.microsoft.com/users/misard/papers/bramble.ps.gz},
	Volume = 2,
	Year = 2001,
	Bdsk-Url-1 = {http://research.microsoft.com/users/misard/papers/bramble.ps.gz}}

@article{isard:98:a,
	Annote = {Primary reference for the CONDENSATION algorithm, a
                  sequential Monte Carlo method for visual
                  tracking. In contrast to tracking techniques based
                  on Kalman filter, this novel approach uses
                  sequential importance sampling for conditional
                  propagation of non-Gaussian probability
                  densities. After introducing the principles of
                  probabilistic object tracking, the authors introduce
                  CONDENSATION for dealing with non-Gaussian
                  observations in temporal image sequences. The
                  article concludes with several practical examples
                  for the strengths of particle fitlering in the
                  context of probabilistic object
                  tracking. Recommended as an entry point for
                  CONDENSATION/particle filter based object tracking.},
	Author = {Michael Isard and Andrew Blake},
	Journal = IJCV,
	Keywords = {CONDENSATION, Probabilistic Object Tracking, Contour-Based Object Tracking},
	Number = 1,
	Pages = {5-28},
	Title = {Condensation -- conditional density propagation for visual tracking},
	Volume = 29,
	Year = 1998}

@inproceedings{isard:98:b,
	Annote = {ICondensation is an extension of the original
                  Condensation algorithm \cite{isard:98:a}. The paper
                  proposes a combination of Condensation with
                  \emph{importance sampling} for increasing the
                  efficiency of probabilistic tracking. In contrast to
                  standard sequential Monte Carlo methods, importance
                  sampling chooses the samples in the resampling step
                  not with respect to their likelihoods but to a
                  so-called importance function. The application in
                  the paper is hand tracking thus skin color is the
                  natural choice as this "guiding" cue for importance
                  sampling. This well-written paper provides an almost
                  natural extension to the original probabilistic
                  tracking algorithm introduced by the same
                  authorship.},
	Author = {Michael Isard and Andrew Blake},
	Booktitle = ECCV,
	Keywords = {Condensation, Particle Filter, Tracking, Importance Sampling, Skin Color},
	Pages = {893-908},
	Title = {{ICondensation}: Unifying low-level and high-level tracking in a stochastic framework},
	Volume = 1,
	Year = 1998}

