@inproceedings{tao:99:a,
	Address = {Corfu, Greece},
	Annote = {This paper proposes an efficient sampling algorithm
                  (SMC) for tracking multiple objects. There are two
                  major contribution: The modeling of multiple objects
                  as object stacks or configurations and the
                  hierarchical sampling scheme for such
                  configurations. Configurations in the sense of this
                  paper are collections of model parameters for
                  multiple object. On one side, this allows to model
                  not only the behaviour of individual objects,
                  e.g. motion but also the behaviour of object groups,
                  in particular addition and deletion of objects. On
                  the other side, stacking multiple objects together
                  increases the problem search space exponentially. In
                  order to overcome the severe computational problems
                  arising from the increased search space, the paper
                  introduces a hierarchical sampling algorithm. In
                  principle, the configurations are broken into
                  sub-configurations according to a subdivison of the
                  image in non-overlapping regions. This smaller
                  sub-configurations are than localy sampled before
                  reassigned to global configurations and propagated
                  globally.},
	Author = {Hai Tao and Harpreet Sawhney and Rakesh Kumar},
	Booktitle = {IEEE Workshop on Vision Algorithms},
	Keywords = {object tracking, particle filter},
	Pages = {53-68},
	Title = {A Sampling Algorithm for Tracking Multiple Objects},
