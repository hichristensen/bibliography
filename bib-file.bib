@inproceedings{sherrah:00:b,
	Annote = {Brief summary paper comparing two approaches to body
                  part tracking: The first approach models the
                  scene-specific a priori knowledge bt means of a
                  Bayesian network, the second approach uses particle
                  filtering with an exclusion principle for tracking
                  the body parts. Both methods are able to resolve
                  uncertainty stemming from ambiguities in visual data
                  but the Bayesian approach usually outperforms the
                  particle filter-based method. This is mainly due to
                  the more sophisticated knowledge of the scenario
                  represented by the Bayes network. Besides this
                  comparison of two methods, this paper provides also
                  a very short introduction/overview of Bayesian
                  networks.},
	Author = {Jamie Sherrah and Shaogang Gong},
