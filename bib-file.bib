@inproceedings{sherrah:00:a,
	Address = {Dublin, Ireland},
	Annote = {This paper presents a head/hand tracker for human
                  computer interfaces (HCI). In contrast to similar
                  systems, this proposal replaces the spatio-temporal
                  prediction for upholding the continuity assumption
                  with Bayesian reasoning about body-part associations
                  based on contextual knowledge. After localizing the
                  subject's head with a mean shift tracker, a Bayesian
                  network s used for reasoning about the most probable
                  association of skin color blobs and hands. Due to
                  this reasoning, the system is able to deal reliably
                  with fast, discontinuous movements of the subject's
                  hands. },
	Author = {Jamie Sherrah and Shaogang Gong},
	Booktitle = ECCV,
	Keywords = {human computer interface, tracking, Bayesian network, mean shift},
	Pages = {150-166},
	Title = {Tracking Discontinuous Motion using Bayesian Inference},
