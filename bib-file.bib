@article{WallisBuelthoff2001,
  Author =	 {G. Wallis and H.H. B{\"u}lthoff},
  Journal =	 {Proceedings of the National Academy of Sciences of
                  the United States of America},
  Note =	 {This paper summarizes experiments done with morphing
                  faces on the temporal aspects of recognition. The
                  experiments show that temporal contiguity is
                  actively used during learning of object models. This
                  presents a nice solution to the 'binding' problem of
