@inproceedings{Williams00isr,
  Abstract =	 {Current work on undersea vehicles at the Australian
                  Centre for Field Robotics concentrates on the
                  development of terrain-aided navigation techniques,
                  sensor fusion and vehicle control architectures for
                  real-time platform control. Accurate position and
                  attitude estimation and control methods use
                  information from scanning sonar to complement a
                  limited vehicle dynamic model and unobservable
                  environmental disturbances. In this paper we present
                  the vehicle control architecture currently running
                  on the Oberon submersible. This architecture is
                  based on the Distributed Architecture for Mobile
                  Navigation. We use a distributed, decoupled control
                  paradigm to facilitate the tuning of individual
                  control modes. A number of behaviours have been
                  created to direct the motion of the vehicle. We show
                  that when coupled with a low-level terrain-aided
                  navigation scheme, effective control of the vehicle
                  can be achieved.},
  Address =	 {Montreal},
  Author =	 {Williams, S.B. and Newman, P. and Dissanayake,
                  M. and Rosenblatt, J. and Durrant-Whyte H.},
  Booktitle =	 {31st Intl. Sym. on Robotics},
  C-Dellaert =	 {Rosenblatt's DAMN applied to Autonomous Underwater
                  Vehicles},
  Date-Added =	 {2014-12-31 17:39:21 +0000},
  Date-Modified ={2014-12-31 17:39:24 +0000},
  Month =	 {May},
  Pages =	 {246-251},
  Title =	 {A decoupled, distributed {AUV} control architecture},
  Volume =	 1,
  Year =	 2000
}

@article{Williams01cviu,
  Abstract =	 {We present a new technique for the simultaneous
                  registration of multiple corresponding point sets
                  with rigid 3D transformations. This class of
                  problems is a generalization of the classic pairwise
                  point set registration task, involving multiple
                  views with multiple correspondences existing between
                  them. The proposed technique requires the
                  computation of a constant matrix which encodes the
                  point correspondence information, followed by an
                  efficient iterative algorithm to compute the optimal
                  rotations. The optimal translations are then
                  recovered directly through the solution of a linear
                  equation system. The algorithm supports weighting of
                  data according to confidence, and we show how it may
                  be incorporated into two robust estimation
                  frameworks to detect and reject outlier data. We
                  have integrated our method into a generalized
                  multiview ICP surface matching system and tested it
                  with synthetic and real data. These tests indicate
                  that the technique is accurate and efficient. The
                  algorithm also compares favorably to another
                  multiview technique from the literature.},
  Author =	 {John Williams and Mohammed Bennamoun},
  Date-Added =	 {2014-12-31 17:39:21 +0000},
  Date-Modified ={2014-12-31 17:39:24 +0000},
  Journal =	 CVIU,
  Number =	 1,
  Pages =	 {117 - 142},
  Title =	 {Simultaneous Registration of Multiple Corresponding
                  Point Sets},
  Volume =	 81,
  Year =	 2001
}

