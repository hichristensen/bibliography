  Author =	 {McCallum, R. Andrew},
  Booktitle =	 {From Animals to Animats, Fourth International
                  Conference on Simulation of Adaptive Behavior,
                  (SAB'96)},
  C-Dellaert =	 {I know this work well, as I tried implementing it
